<div class="tab-content" id="orders-table-tab-content">     <div class="tab-pane fade show active" id="orders-all" role="tabpanel" aria-labelledby="orders-all-tab">         <div class="app-card app-card-orders-table shadow-sm mb-5">             <div class="app-card-body">                 <div class="search-container">                     <input type="text" id="orNumberInput" class="form-control" placeholder="Enter OR Number">                     <button onclick="searchORNumber()" class="btn btn-primary ml-2">Search</button>                 </div>                 <div class="row">                     <div class="col-lg-12">                         <h3>Stocks <a href="admin/#" class="btn btn-primary btn-sm  "> <i                                     class="fa fa-plus-circle fw-fa"></i> New</a> </h3>                     </div>                  </div>                  <div class="table-responsive">                     <table class="table app-table-hover mb-0 text-left">                         <thead>                             <tr>                                 <th class="cell">OR Number</th>                                 <th class="cell">Products and Quantity</th>                                 <th class="cell">Total Price</th>                              </tr>                         </thead>                         <tbody id="orderTableBody">                             <tr>                                 <td colspan="3" class="text-center">No data available</td>                             </tr>                         </tbody>                     </table>                 </div>             </div>         </div>     </div> </div>  <script> function searchORNumber() {     const orNumber = document.getElementById('orNumberInput').value;     const tableBody = document.getElementById('orderTableBody');      tableBody.innerHTML = '';      fetch('fetch_orders.php?orNumber=' + orNumber)         .then(response => response.json())         .then(data => {             if (data.length > 0) {                 data.forEach(order => {                     const row =                          <tr>                             <td>${orNumber}</td>                             <td>${order.productName} (${order.productStock})</td>                             <td>${order.productPrice}</td>                         </tr>                     ;                     tableBody.innerHTML += row;                 });             } else {                 const noDataRow =                      <tr>                         <td colspan="3" class="text-center">No data found for OR Number: ${orNumber}</td>                     </tr>                 ;                 tableBody.innerHTML = noDataRow;             }         })         .catch(error => {             console.error('Error fetching data:', error);             const errorRow =                  <tr>                     <td colspan="3" class="text-center">Error fetching data for OR Number: ${orNumber}</td>                 </tr>             ;             tableBody.innerHTML = errorRow;         }); } </script>
